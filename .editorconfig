root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_size = 4

[*.go]
indent_size = 4

[*.sh]
indent_size = 4

[*.yml]
indent_size = 2

[*.cc]
indent_size = 4

[*.h]
indent_size = 4

[*.c]
indent_size = 4

[*.js]
indent_size = 4

[*.ts]
indent_size = 4

[*.tsx]
indent_size = 4

[*.jsx]
indent_size = 4

[*.cs]
indent_size = 4

[*.java]
indent_size = 4

[Makefile]
indent_style = tab
